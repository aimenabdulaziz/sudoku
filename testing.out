chmod a+x testing.sh
bash -v ./testing.sh
#!/bin/bash
#
# testing.sh - perform testing for sudoku
#
# usage: run 'make test' and redirect to a directory as needed 
#
# ./sudoku command

# make Sudoku
make
gcc -Wall -pedantic -std=c11 -ggdb    -c -o sudoku.o sudoku.c
gcc -Wall -pedantic -std=c11 -ggdb    -c -o create.o create.c
gcc -Wall -pedantic -std=c11 -ggdb    -c -o solve.o solve.c
gcc -Wall -pedantic -std=c11 -ggdb  sudoku.o create.o solve.o -o sudoku
gcc -Wall -pedantic -std=c11 -ggdb  -DCREATETEST create.c solve.c -o create
gcc -Wall -pedantic -std=c11 -ggdb  -DSOLVETEST solve.c create.c -o solve
gcc -Wall -pedantic -std=c11 -ggdb    -c -o fuzzSudoku.o fuzzSudoku.c
gcc -Wall -pedantic -std=c11 -ggdb  fuzzSudoku.o create.o solve.o -o fuzzSudoku

# incorrect command line arguments
./sudoku solve and create
usage: ./sudoku command, where command is solve or create

################ creator ################
# generate a random sudoku 5 time and solve them
dir=tests
for i in {1..3}; do
    # create a new random sudoku and save it
    ./sudoku create > "$dir/create$i"

    # wait 1 second between each iteration to generate a different sudoku as srand depends on time
    sleep 1 
done

